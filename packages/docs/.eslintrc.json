{
    "extends": [
        "@thiagom790-ignite-ui/eslint-config",
        "plugin:storybook/recommended"
    ],
    "ignorePatterns": [
        "dist",
        ".eslintrc.cjs"
    ],
    "plugins": [
        "react-refresh"
    ],
    "rules": {
        "react-refresh/only-export-components": [
            "warn",
            {
                "allowConstantExport": true
            }
        ]
    }
}